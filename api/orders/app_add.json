{
    "name": "添加/修改项目",
    "method": "post",
    "params": {
        "id": {
            "lbl": "项目ID",
            "type": "int"
        },
        "title": {
            "lbl": "项目名称",
			"len": [0, 16]
		},
        "url": {
			"lbl": "发货接口",
			"rem": "http://xxx.com?foo=bar 收到请求格式 http://xxx.com?foo=bar&ext=xxx&r=xxx&s=sign",
			"len": [0, 256]
        },
        "cer": {
			"lbl": "签名",
			"len": [0, 32]
        },
        "back": {
            "lbl": "支付后跳转链接",
			"len": [0, 256]
        }
	},
	"grant":"{U}",
	"check":[
		{"R": "{}.id||{}.title!=null", "M": "创建项目时必须设置项目名称"}
	],
	"error": {
		"404": "项目不存在或无权限",
		"405": "已存在相同名称的项目",
        "406": "存在同名项目"
	},
	"ret": {
		"no": 200
	}
}