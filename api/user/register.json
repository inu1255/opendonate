{
    "name": "注册",
    "method": "POST",
    "params": {
        "name": {
            "lbl": "用户名"
        },
        "account": {
            "lbl": "账号",
            "need": true,
            "reg": "^\\S{3,32}$"
        },
        "title": {
            "lbl": "邮箱/手机",
            "need": true,
            "reg": "^(1\\d{10}|[^@]+@[^\\.]+\\.\\S+)$"
        },
        "passwd": {
            "lbl": "密码",
            "need": true,
            "len": [ 6, 32 ]
        },
        "code": {
            "lbl": "验证码",
            "need": true,
            "reg": "^\\w{4,8}$"
        },
        "invite": {
            "lbl": "邀请码"
        }
    },
    "error": {
        "405": "邮箱/手机被占用",
        "406": "验证码错误",
        "407": "验证码已过期,请重新发送验证码",
        "408": "账号已存在",
        "409": "邀请码无效"
    },
    "ret": {
        "no": 200,
        "data": {
            "id": 1026,
            "email": "003@qq.com",
            "account": "@@003",
            "money": 200,
            "invite_code": "%1NgMTAy4%%"
        }
    }
}