{
    "name": "添加用户",
    "method": "POST",
    "params": {
        "name": {
            "lbl": "姓名"
		},
		"account": {
			"lbl": "账号"
		},
        "email": {
            "lbl": "邮箱",
			"reg": "^[\\w-]+@[\\w-]+(.[\\w-]+)+$"
		},
        "tel": {
            "lbl": "电话号码",
            "reg": "^1\\d{10}$"
        },
        "passwd": {
            "lbl": "密码",
            "len": [6, 32]
		},
        "avatar":{
            "lbl": "头像url"
        },
        "profile": {
            "lbl": "个人介绍"
		},
		"role": {
			"lbl": "角色"
		}
	},
	"grant": "{U}.lvl<1", // 管理员才能使用
	"check":[
		{"R": "{}.account||{}.email||{}.tel", "M": "账号/手机号/邮箱必传其一"}
	],
    "error": {
		"405": "账号不存在"
    },
    "ret": {
		"no": 200,
		"data": { // 仅添加用户时返回
			"id": 1 // 用户ID
		}
    }
}